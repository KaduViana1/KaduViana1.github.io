<style>
  .background {
    background-color: rgb(24, 24, 27);
    background-size: 100% 100%;
  }

  .back-shapes {
    width: 100%;
    height: 100%;
    position: relative;
  }

  .icon {
    position: absolute;
    animation-name: icon;
    -webkit-animation-name: icon;
    animation-duration: 5s;
    -webkit-animation-duration: 5s;
    animation-iteration-count: infinite;
    -webkit-animation-iteration-count: infinite;
    display: inline-block;
    width: 2.8rem;
    height: 2.8rem;
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center center;
  }

  .js {
    background-image: url(/icons/js.png);
  }
  .react {
    background-image: url(/icons/react.png);
  }
  .ts {
    background-image: url(/icons/ts.png);
  }
  .vue {
    background-image: url(/icons/vue.png);
  }
  .next {
    background-image: url(/icons/nextjs.png);
  }
  .docker {
    background-image: url(/icons/docker.png);
  }
  .css {
    background-image: url(/icons/css.png);
  }
  .html {
    background-image: url(/icons/html.png);
  }
  .node {
    background-image: url(/icons/node.png);
  }
  .mongo {
    background-image: url(/icons/mongodb.png);
    width: 60px;
    height: 60px;
  }
  .git {
    background-image: url(/icons/git.png);
    width: 50px;
    height: 50px;
  }
  .sql {
    background-image: url(/icons/mysql.png);
    width: 50px;
    height: 50px;
  }
  .nuxt {
    background-image: url(/icons/nuxt.png);
  }
  .redux {
    background-image: url(/icons/redux.png);
  }
  .sass {
    background-image: url(/icons/sass.png);
  }
  .tailwind {
    background-image: url(/icons/tailwind.png);
  }
  .astro {
    background-image: url(/icons/astro.png);
    width: 3.5rem;
    height: 3.5rem;
  }
  .express {
    background-image: url(/icons/express.png);
  }

  @keyframes icon {
    0% {
      transform: translateY(0%) rotate(-55deg);
    }
    50% {
      transform: translateY(400%) rotate(55deg);
    }
    100% {
      transform: translateY(0%) rotate(-55deg);
    }
  }

  @-webkit-keyframes icon {
    0% {
      -webkit-transform: translateY(0%);
    }
    50% {
      -webkit-transform: translateY(400%);
    }
    100% {
      -webkit-transform: translateY(0%);
    }
  }
</style>
